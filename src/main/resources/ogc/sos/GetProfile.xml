		<prop name="req.05.useBothCoverageAndSweCommon.coverage" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseBothCoverageAndSweCommon">true</prop>
		<prop name="req.06.useTimeSeries" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseTimeSeries">planned</prop>
		<prop name="req.07.useFOIReference" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseFOIReference">planned</prop>
		<prop name="req.08.useReturnFOINotCollection" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseReturnFOINotCollection">not yet implemented</prop>
		<prop name="req.09.useEPSGPattern" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseEPSGPattern">in progress</prop>
		<prop name="req.10.useEPSGAxisOrder" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseEPSGAxisOrder">in progress</prop>
		<prop name="req.11.useOMParameter" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseOMParameter">still working out what this means</prop>
