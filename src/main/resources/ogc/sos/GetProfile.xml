<props>
	<!-- 
		This is a experimental tentative extension to OGC's services. If adopted, this will need to go to
		OWS. 
		
		The intent of this extension is to flesh out the meaning of profile in a machine-actionable way.
		The current way of defining profiles for community use via text documents does not enhance 
		real world interoperability. In order to connect implementations of OGC services, implementers
		have to do tons of reading of text documents which are not even properly hyperlinked together.
		This is impossible for a machine to do, and extremely time consuming for a human implementer to
		do, and in addition, the human implementer must then program the rules into the implementation
		after reading.
		
		

	 -->
	<propGroup name="GWIE_SOS_Implementation_Profile" version="0.1" prefix="net.opengis.sos.1_0.profile.gwie" extends="">
		<prop name="req.01.useSos_1_0" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseSos">true</prop>
		<prop name="req.02.useHttpURI" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseHttpURI">false, to read</prop>
		<prop name="req.02.useHttpURI.resolvable" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseHttpURI">false, to read</prop>
		<prop name="req.02.useHttpURI.negotiated" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseHttpURI">false, to read</prop>
		<prop name="req.03.useSingleCollection" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseSingleCollection">false</prop>
		<prop name="req.04.useWaterML2" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseWaterML2">maybe, check</prop>
		<prop name="req.05.useBothCoverageAndSweCommon" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseBothCoverageAndSweCommon">false</prop>
		<prop name="req.05.useBothCoverageAndSweCommon.sweCommon" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseBothCoverageAndSweCommon">false</prop>
		<prop name="req.05.useBothCoverageAndSweCommon.coverage" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseBothCoverageAndSweCommon">true</prop>
		<prop name="req.06.useTimeSeries" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseTimeSeries">true</prop>
		<prop name="req.07.useFOIReference" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseFOIReference">true, but currently FOI is not returned in the response</prop>
		<prop name="req.08.useReturnFOINotCollection" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseReturnFOINotCollection">true, </prop>
		<prop name="req.09.useEPSGPattern" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseEPSGPattern">false, Jordan to correct</prop>
		<prop name="req.10.useEPSGAxisOrder" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseEPSGAxisOrder">false, Jordan to correct</prop>
		<prop name="req.11.useOMParameter" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseOMParameter">false, Jordan to correct</prop>
		<prop name="req.12.useSpatialAndTemporalFilter" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseSpatialAndTemporalFilter">not yet determined</prop>
		<prop name="req.13.useIntelligentLocation" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseIntelligentLocation">true</prop>
