		<prop name="req.02.useHttpURI.resolvable" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseHttpURI">planned</prop>
		<prop name="req.02.useHttpURI.negotiated" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseHttpURI">false</prop>
		<prop name="req.02.useHttpURI.wfs" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseHttpURI">planned</prop>
		<prop name="req.03.useSingleCollection" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseSingleCollection">in discussion, consolidate Eric B. nested featureCollection with Dave B's derivedOffering</prop>
		<prop name="req.04.useWaterML2" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseWaterML2">planned</prop>
		<prop name="req.04.useWaterML2.schema" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseWaterML2">planned</prop>
		<prop name="req.04.useWaterML2.profile" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseWaterML2">planned</prop>
		<prop name="req.05.useBothCoverageAndSweCommon" docRef="http://external.opengis.org/twiki_public/bin/view/HydrologyDWG/GwIeSOSProfile#UseBothCoverageAndSweCommon">maybe</prop>
